{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${mod_name}",
	"description": "Change me!",
	"authors": [
		"mafien0"
	],
	"contact": {
		"sources": "https://github.com/${github_repo}"
	},
	"license": "CC0-1.0",
	"icon": "assets/template-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "cc.mafien0.template.TemplateMod",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "cc.mafien0.template.TemplateModClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"template-mod.mixins.json",
		{
			"config": "template-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=${fabricloader}",
		"minecraft": "~${minecraft}",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}